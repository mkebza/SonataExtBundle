<ol class="nav navbar-top-links breadcrumb">
    <li>
        <a href="{{ path('sonata_admin_dashboard') }}">
            <i class="fa fa-home"></i>
        </a>
    </li>
    {% for item in items %}
        {% if loop.last %}
            <li class="active"><span>{{ item.label }}</span></li>
        {% else %}
            <li><a href="{{ item.url }}">{{ item.label }}</a></li>
        {% endif %}
    {% endfor %}

</ol>
